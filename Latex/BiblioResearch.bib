@article{lee2013happens,
	Abstract = {While many studies have investigated the determinants of housing demand, very few studies have focused on how economic conditions affect the formation of potential households directly. Potential households may choose to delay entry into the housing market by remaining with one's parents during times of economic hardship or by combining with other persons to share housing costs. Using a variety of modeling approaches, we find that both the increase in the unemployment rate and the presence of recessions reduce the rate of household formation. Simulations suggest that these declines are substantively important. For example, in a recession, the likelihood that a young adult will form an independent household falls by 1--9% points depending on the age of the person. By way of comparison, if an individual is unemployed, the likelihood of leaving the parental home is up to 11% points lower.},
	Annote = {Closest to what I am trying to do for Australia. Good base model and uses simulation also.

``Research has also found that demographic characteristics of children, such as gender (Murphy and Wang, 1998) and race (Goldscheider and DaVanzo, 1989), and family structure (Goldscheider and Goldscheider, 1998 and Murphy and Wang, 1998) predict the timing of when young adults establish independence.''

``The current evidence is also inconclusive concerning the impact of housing prices and rents on the timing of household formation. ''- proceeds to list research on the topic.

At present, there are only two papers that test whether local labor market conditions influence household formation rates. 

Uses Heckman style selection model.
},
	Author = {Lee, Kwan Ok and Painter, Gary},
	Date-Added = {2017-08-17 23:49:12 +0000},
	Date-Modified = {2017-08-24 23:59:44 +0000},
	Journal = {Journal of Urban Economics},
	Pages = {93--109},
	Publisher = {Elsevier},
	Read = {1},
	Title = {What happens to household formation in a recession?},
	Volume = {76},
	Year = {2013},
	Bdsk-File-1 = {}}

@techreport{Banks2015House,
	Abstract = {This paper investigates the effects of spatial housing price risk on housing choices over the first half of the life-cycle. Housing price risk can be substantial but, unlike other risky assets which people can avoid, most people want to eventually own their home thereby creating an insurance demand early in life. Our contribution focuses on the importance of home ownership as a hedge against future house price risk for individuals that plan to move up the housing ladder. We use a simple theoretical model to show that people living in places with higher housing price risk should own their first home at a younger age, live in larger homes, and be less likely to refinance. These predictions are shown to hold using panel data from the United States and United Kingdom.},
	Annote = {Argument is that unlike other risky goods, early house purchase acts as insurance against future house price increases in particular in more volatile markets. Analysis uses a probit model applied to UK and US housing up to the 1990s. The argument is well supported by this data as well as by a numeric simulation based on a model. },
	Author = {James Banks and Richard Blundell and Zo{\'e} Oldfield and James P. Smith},
	Date-Added = {2017-08-17 01:33:18 +0000},
	Date-Modified = {2017-08-25 00:00:45 +0000},
